{
  "name": "node-red-desktop",
  "version": "0.0.1",
  "main": "main.js",
  "build": {
    "appId": "com.electron.node-red",
    "productName": "Node-RED",
    "files": [
      "**/*",
      {
        "from": "node_modules",
        "to": "node_modules"
      }
    ],
    "win": {
      "target": [
        {
          "target": "msi"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg"
        }
      ]
    },
    "linux": {
      "category": "Development",
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "armv7l"
          ]
        },
        {
          "target": "rpm"
        }
      ],
      "publish": []
    }
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "dependencies": {
    "node-red": "1.1.3",
    "node-red-dashboard": "^2.23.3"
  },
  "devDependencies": {
    "electron": "^10.1.1",
    "electron-builder": "^22.8.0"
  },
  "description": "Node-RED Desktop",
  "keywords": [
    "node-red",
    "desktop",
    "standalone",
    "electron"
  ],
  "license": "Apache-2.0",
  "homepage": "https://nodered.org",
  "contributor": [
    "Kazuhito Yokoi",
    "Dave Conway-Jones",
    "Nick O'Leary"
  ],
  "author": "Node-RED <team@nodered.org>"
}
